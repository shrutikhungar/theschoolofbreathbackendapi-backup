{
    "version": 2,
    "builds": [
      {
        "src": "src/index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "src/index.js"
      }
    ],
    "functions": {
      "api/*.js": {
        "memory": 3008,
        "maxDuration": 30
      }
    }
    
  }